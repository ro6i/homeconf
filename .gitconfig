# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
  name = Ruben Ch8n
  email = 

[alias]
  s = status
  logs = log --graph --abbrev-commit --decorate --format=format:'%C(blue)%h%C(reset) - %C(magenta)%aD%C(reset) %C(green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)'
  last = log -1 HEAD

[difftool]
    prompt = false
[diff]
    tool = nvimdiff
[difftool "nvimdiff"]
    cmd = "nvim -d \"$LOCAL\" \"$REMOTE\""
